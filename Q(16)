import java.util.Scanner;
public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.print("day: ");
		String day=sc.next().toLowerCase();
		if(day.equals("monday"))System.out.println("THE ticket price is 50");
		else if(day.equals("tueday"))System.out.println("THE ticket price is 50");
		else if(day.equals("wednesday"))System.out.println("THE ticket price is 50");
		else if(day.equals("thursday"))System.out.println("THE ticket price is 50");
		else if(day.equals("friday"))System.out.println("THE ticket price is 50");
		else if(day.equals("saturday"))System.out.println("THE ticket price is 100");
		else if(day.equals("sunday"))System.out.println("THE ticket price is 100");
		else System.out.println("no day");
	}
}
